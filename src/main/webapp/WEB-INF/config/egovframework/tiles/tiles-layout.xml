<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
    "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
    "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
 
<tiles-definitions>
     
    <definition name="default" template="/WEB-INF/jsp/egovframework/tiles/layout/default_layout.jsp" >
        <put-attribute name="header" value="/WEB-INF/jsp/egovframework/tiles/header.jsp" />
        <put-attribute name="footer" value="/WEB-INF/jsp/egovframework/tiles/footer.jsp" />
    </definition>
    
    <definition name="/*/*"  extends="default">
        <put-attribute name="content"    value="/WEB-INF/jsp/egovframework/survey/{1}/{2}.jsp"/>
    </definition>
     
     
</tiles-definitions>


